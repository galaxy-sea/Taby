{
  "name": "Taby",
  "manifest_version": 3,
  "version": "1.0.9",
  "description": "A simple new tab extension",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "author": {
    "email": "ganpeng0119@gmail.com"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["/content/content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["favicon", "storage", "tabs", "activeTab"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  "icons": {
    "32": "icons/32.png",
    "72": "icons/72.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "action": {
    "default_title": "Taby",
    "default_icon": "icons/32.png",
    "default_popup": "popup.html"
  }
}
