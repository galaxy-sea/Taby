{
  "name": "tabs",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "this is tabs",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "author": {
    "email": "190875565@qq.com"
  },
  "permissions": [
    "favicon",
    "identity",
    "storage",
    "tabs",
    "activeTab"
  ],
  "background": {
    "service_worker": "src/background/index.js",
    "type": "module"
  },
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"],
      "extension_ids": ["*"]
    }
  ],
  "icons": {
    "32": "src/assets/32.png",
    "72": "src/assets/72.png",
    "128": "src/assets/128.png",
    "512": "src/assets/512.png"
  }
}
